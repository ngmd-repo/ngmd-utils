{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "library-ng": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "type": "Component",
          "style": "scss",
          "skipTests": true,
          "changeDetection": "OnPush"
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true,
          "type": "Directive"
        },
        "@schematics/angular:guard": {
          "typeSeparator": ".",
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "typeSeparator": ".",
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "typeSeparator": ".",
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "typeSeparator": ".",
          "skipTests": true
        },
        "@schematics/angular:service": {
          "type": "Service",
          "skipTests": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "ng",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist/library-ng"
            },
            "index": "src/index.html",
            "polyfills": [
              "src/polyfills.ts"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": []
            },
            "scripts": [],
            "browser": "src/main.ts",
            "allowedCommonJsDependencies": [
              "@ng-doc/core"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "library-ng:build:production"
            },
            "development": {
              "buildTarget": "library-ng:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "library-ng:build"
          }
        }
      }
    },
    "utils": {
      "projectType": "library",
      "root": "projects/utils",
      "sourceRoot": "projects/utils/src",
      "schematics": {
        "@schematics/angular:component": {
          "type": "Component",
          "style": "scss",
          "skipTests": true,
          "changeDetection": "OnPush"
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true,
          "type": "Directive"
        },
        "@schematics/angular:guard": {
          "typeSeparator": ".",
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "typeSeparator": ".",
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "typeSeparator": ".",
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "typeSeparator": ".",
          "skipTests": true
        },
        "@schematics/angular:service": {
          "type": "Service",
          "skipTests": true
        }
      },
      "prefix": "ng",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/utils/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/utils/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/utils/tsconfig.lib.json",
              "watch": true
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/**/*.html",
              "projects/**/*.ts",
              "src/**/*.html",
              "src/**/*.ts"
            ]
          }
        }
      }
    },
    "docs": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "standalone": true,
          "type": "Component",
          "changeDetection": "OnPush",
          "skipTests": true,
          "skipImport": true
        },
        "@schematics/angular:directive": {
          "type": "Directive",
          "standalone": true,
          "skipTests": true,
          "skipImport": true
        },
        "@schematics/angular:pipe": {
          "typeSeparator": ".",
          "standalone": true,
          "skipTests": true,
          "skipImport": true
        }
      },
      "root": "projects/docs",
      "sourceRoot": "projects/docs/src",
      "prefix": "ng",
      "architect": {
        "build": {
          "builder": "@ng-doc/builder:application",
          "options": {
            "allowedCommonJsDependencies": [
              "@ng-doc/core",
              "dayjs",
              "@braintree/sanitize-url",
              "dompurify",
              "cytoscape-cose-bilkent",
              "elkjs/lib/elk.bundled.js",
              "dayjs/plugin/isoWeek.js",
              "dayjs/plugin/customParseFormat.js",
              "dayjs/plugin/advancedFormat.js",
              "@ng-doc/core/helpers/as-array",
              "@ng-doc/core/helpers/object-keys",
              "@ng-doc/core/helpers/is-present",
              "@ng-doc/core/helpers/is-same-object",
              "@ng-doc/core/helpers/extract-function-defaults",
              "@ng-doc/core/helpers/build-playground-demo-template",
              "@ng-doc/core/helpers/stringify",
              "@ng-doc/core/helpers/extract-value",
              "@ng-doc/core/helpers/is-keyboard-event"
            ],
            "outputPath": "dist/docs",
            "index": "projects/docs/src/index.html",
            "browser": "projects/docs/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/docs/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "assets/",
                "output": "assets/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@ng-doc/ui-kit/assets",
                "output": "assets/ng-doc/ui-kit"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@ng-doc/app/assets",
                "output": "assets/ng-doc/app"
              },
              {
                "glob": "**/*",
                "input": "projects/docs/ng-doc/docs/assets",
                "output": "assets/ng-doc"
              }
            ],
            "styles": [
              "node_modules/@ng-doc/app/styles/global.css",
              "projects/docs/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "ngDoc": {
                "config": "./projects/docs/ng-doc.config.ts"
              },
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3MB",
                  "maximumError": "5MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "1MB"
                }
              ],
              "outputHashing": "all",
              "progress": false
            },
            "development": {
              "ngDoc": {
                "config": "./projects/docs/ng-doc.config.ts"
              },
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@ng-doc/builder:dev-server",
          "options": {
            "port": 4300
          },
          "configurations": {
            "production": {
              "buildTarget": "docs:build:production"
            },
            "development": {
              "buildTarget": "docs:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/docs/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/docs/public"
              }
            ],
            "styles": [
              "projects/docs/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "docs-en": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/docs-en",
      "sourceRoot": "projects/docs-en/src",
      "prefix": "ng",
      "architect": {
        "build": {
          "builder": "@ng-doc/builder:application",
          "options": {
            "allowedCommonJsDependencies": [
              "@ng-doc/core",
              "dayjs",
              "@braintree/sanitize-url",
              "dompurify",
              "cytoscape-cose-bilkent",
              "elkjs/lib/elk.bundled.js",
              "dayjs/plugin/isoWeek.js",
              "dayjs/plugin/customParseFormat.js",
              "dayjs/plugin/advancedFormat.js",
              "@ng-doc/core/helpers/as-array",
              "@ng-doc/core/helpers/object-keys",
              "@ng-doc/core/helpers/is-present",
              "@ng-doc/core/helpers/is-same-object",
              "@ng-doc/core/helpers/extract-function-defaults",
              "@ng-doc/core/helpers/build-playground-demo-template",
              "@ng-doc/core/helpers/stringify",
              "@ng-doc/core/helpers/extract-value",
              "@ng-doc/core/helpers/is-keyboard-event"
            ],
            "browser": "projects/docs-en/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/docs-en/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "assets/",
                "output": "assets/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@ng-doc/ui-kit/assets",
                "output": "assets/ng-doc/ui-kit"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@ng-doc/app/assets",
                "output": "assets/ng-doc/app"
              },
              {
                "glob": "**/*",
                "input": "projects/docs-en/ng-doc/docs/assets",
                "output": "assets/ng-doc"
              }
            ],
            "styles": [
              "node_modules/@ng-doc/app/styles/global.css",
              "projects/docs-en/src/styles.scss"
            ]
          },
          "configurations": {
            "production": {
              "ngDoc": {
                "config": "./projects/docs-en/ng-doc.config.ts",
                "progress": false
              },
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3MB",
                  "maximumError": "5MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "1MB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "ngDoc": {
                "config": "./projects/docs-en/ng-doc.config.ts"
              },
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@ng-doc/builder:dev-server",
          "options": {
            "port": 4400
          },
          "configurations": {
            "production": {
              "buildTarget": "docs-en:build:production"
            },
            "development": {
              "buildTarget": "docs-en:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/docs-en/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/docs-en/public"
              }
            ],
            "styles": [
              "projects/docs-en/src/styles.scss"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
